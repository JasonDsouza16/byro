<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title><%= org_name %> - <%= branch_location %> - Album</title>
    <link rel='shortcut icon' type='image/x-icon' href="https://res.cloudinary.com/byro/image/upload/v1632035797/public/favicon.ico"/>
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Fredericka+the+Great" rel="stylesheet">
		<!-- Bootstrap -->
		<link rel="stylesheet" href="https://res.cloudinary.com/byro/raw/upload/v1632035798/public/css/bootstrap.css">
		<!-- Animate.css -->
		<link rel="stylesheet" href="https://res.cloudinary.com/byro/raw/upload/v1632035797/public/css/animate.css">
		<!-- Default stylesheet -->
		<link rel="stylesheet" href="https://res.cloudinary.com/byro/raw/upload/v1632649808/public/css/main.css">
  </head>
  <body id="top">
    <header>
      <!-- NAVBAR -->
      <nav class="navbar navbar-expand-lg navbar-light bg-light fixed-top">
				<a class="navbar-brand animated pulse" href="#">
				<img src="<%= org_logo %>" width="30" height="30" alt="Logo">
				</a>
				<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
				<span class="navbar-toggler-icon"></span>
				</button>
				<div class="collapse navbar-collapse text-center" id="navbarNav">
					<ul class="navbar-nav">
						<li class="nav-item active">
							<a class="nav-link" href="../">Home</a>
						</li>
            <li class="nav-item">
							<a class="nav-link" href="/o/<%= org_name %>/#br_list">Branches</a>
						</li>
						<li class="nav-item dropdown">
							<a class="nav-link dropdown-toggle" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
							Menu
							</a>
							<div class="dropdown-menu text-center text-lg-left" aria-labelledby="navbarDropdown">
								<a class="dropdown-item" href="/o/<%= org_name %>/<%= branch_location %>/menu/veg">Veg</a>
                <a class="dropdown-item" href="/o/<%= org_name %>/<%= branch_location %>/menu/non-veg">Non-Veg</a>
							</div>
						</li>
            <li class="nav-item">
              <a class="nav-link" href="#about_us_section">About</a>
            </li>
						<li class="nav-item">
							<a class="nav-link" href="#">Album<span class="sr-only">(current)</span></a>
						</li>
						<li class="nav-item">
							<a class="nav-link" href="#contact">Contact</a>
						</li>
					</ul>
					<div class="ml-auto">
						<a class="nav-social mr-2" href="<%= branch_facebook %>"><i class="fa fa-facebook" aria-hidden="true"></i></a>
						<a class="nav-social mr-2" href="<%= branch_instagram %>"><i class="fa fa-instagram" aria-hidden="true"></i></a>
						<a class="nav-social" href="mailto:<%= branch_email %>"><i class="fa fa-envelope" aria-hidden="true"></i></a>
					</div>
				</div>
			</nav>
    <% if (messages.error) { %>
      <div class="alert alert-danger" role="alert"><%- messages.error %></div>
    <% } %>
    </header>
    <main>

      <!-- GALLERY DESCRIPTION -->
      <div class="gallery-description container-fluid bg-dark text-white">
        <h1 id="title-2" class="special-title-2">FOOD GALLERY</h1>
      </div>

      <!-- GALLERY THUMBNAILS -->
      <div id="gallery-thumbnails" class="gallery-thumbnails bg-light">
        <div class="container">
          <div class="row">
          
            <% data.forEach(dish => { %>
            <div class="gallery-item col-md-4">
                <img class="img-fluid rounded box-shadow border" src=<%= dish[1] %> srcset="<%= dish[2]%>" draggable="false" alt="Food">
              </a>
            </div>
            <% }) %>
          </div>
        </div>
      </div>
      <div class="spacer container-fluid"></div>
    </main>

    <footer class="page-footer" id="contact">
			<div class="container">
				<a class="navbar-brand animated pulse d-block text-center m-0 p-0" href="/o/<%= org_name %>/">
				<img src="<%= org_logo %>" width="50" height="50" alt="Logo">
				</a>
				<div class="row">
					<div class="col-md-4">
						<h5 class="page-footer-title">TIMINGS</h5>
						<p class="mb-0 p-line"><%= branch_timings %></p>
					</div>
					<div class="col-md-4 h-100 border-left-custom">
						<h5 class="page-footer-title mt-3 mt-md-0">LOCATION</h5>
						<address class="p-line">
              <%= branch_address %><br>
							<abbr title="Phone">P:</abbr><%= branch_contact %><br>
							<abbr title="Email">E:</abbr><%= branch_email %>
						</address>
					</div>
					<div class="col-md-4 h-100 border-left-custom">
						<h5 class="page-footer-title mt-3 mt-md-0">FOLLOW US</h5>
						<div class="mb-3">
							<a class="footer-social" href="<%= branch_facebook %>" aria-label="facebook"><i class="fa fa-facebook" aria-hidden="true"></i></a>
							<a class="footer-social" href="<%= branch_instagram %>" aria-label="instagram"><i class="fa fa-instagram" aria-hidden="true"></i></a>
						</div>
					</div>
				</div>
			</div>
			<div class="page-footer-copyright container-fluid text-center">
				Â© 2021 Byro's Family Restaurant. All rights reserved.<br>
        Design & Development by S.S.R.J.
			</div>
		</footer>
    <a id="back-to-top" class="back-to-top smooth-scroll" href="#top">
      <i class="fa fa-long-arrow-up" aria-hidden="true"></i>
    </a>
    <!-- jQuery -->
    <script
      src="https://code.jquery.com/jquery-3.3.1.min.js"
      integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
      crossorigin="anonymous">
    </script>
    <!-- Bootstrap -->
    <script 
      src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" 
      integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" 
      crossorigin="anonymous">
    </script>
    <script src="https://res.cloudinary.com/byro/raw/upload/v1632035798/public/js/site.js"></script>
    <script src="https://res.cloudinary.com/byro/raw/upload/v1632035798/public/js/gallery-animations.js"></script>
  </body>
</html>
    